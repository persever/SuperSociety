<h3>New Group</h3>

<form>

  <label>Name</label>
  <input type="text" name="group[name]" value="<%= group.escape("name") %>">
  <br>

  <label>Description</label>
  <textarea name="group[description]" value="<%= group.escape("description") %>"></textarea>

  <input type="submit" value="Next">
</form>
