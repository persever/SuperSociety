<div class="pull-left">
  <div class="creator-buttons-container">
    <div class="creator-buttons">
      <div class="group-delete-button"></div>
      <div class="group-edit-button"></div>
    </div>
  </div>

  <div class="img-container">
    <img class="main-img" src="<%= group.escape("photo_url") %>"></img>
  </div>
  <div class="group-description"><%= group.escape("description") %></div>

  <div class="subscribers-container">
    <div class="header-bar members">
      <h3>Members</h3>
    </div>
    <div class="subscribers">
    </div>
  </div>
</div>

<div class="title-and-events">
  <div class="title-container">
      <h2 class="name"><%= group.escape("name") %><div class="hover-glow"></div></h2>
    <div class="subscription-button pull-right"></div>
  </div>
  <div class="group-events"></div>
</div>
